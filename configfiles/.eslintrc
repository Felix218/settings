{
    "parser": "babel-eslint",
    "extends":[
       "prettier",
       "prettier/react",
       "plugin:react/recommended"
    ],
    "plugins":[
       "prettier",
       "react"
    ],
    "parserOptions":{
       "ecmaVersion":2016,
       "sourceType":"module",
       "ecmaFeatures":{
          "jsx":true
       }
    },
    "env":{
       "es6":true,
       "node":true
    },
    "rules":{
       "prettier/prettier":"error",
       "no-unused-vars": "error",
       "react/prop-types": 0,
       "react/display-name": 0
    }
 }